{
  "name": "pr-approval-finder",
  "version": "5.0.0",
  "description": "Find minimum required approvals for GitHub Pull Requests",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "cd server && npm start"
  },
  "keywords": [
    "github",
    "pr",
    "approval",
    "codeowners",
    "review"
  ],
  "author": "Aswin",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
} 